<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
											
<mapper namespace="com.project.gwasil_zero.mapper.MemberMapper">
										<!--이부분 꼭 수정하자-->
	<select id="getMember" parameterType="hashmap" resultType="com.project.gwasil_zero.model.User">
		SELECT *
		FROM TBL_USER
		WHERE USER_ID = #{userId} AND USER_PASSWORD = #{pwd}
	</select>
</mapper>